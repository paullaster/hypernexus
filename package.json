{
  "name": "@brainspore/hypernexus",
  "version": "6.6.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "start": "ts-node --loader ts-node/esm test.ts",
    "build": "tsc --build --clean && tsc --build",
    "watch": "tsc --watch",
    "test:unit": "vitest run",
    "test:e2e": "vitest run --env=e2e",
    "test": "tsx tests/unit/oauth2.test.mts",
    "generate-docs": "typedoc --out docs ",
    "lint": "eslint src -c js,mjs,cjs,ts",
    "format": "prettier --write 'src/**/*.{js,mjs,cjs,ts}'",
    "release": "zx release.js",
    "release:patch": "zx release.js",
    "release:minor": "zx release.js --minor",
    "release:major": "zx release.js --major"
  },
  "keywords": [
    "Windows Authentication",
    "Nodejs",
    "Network Transport Wrapper"
  ],
  "author": "Paullaster Okoth",
  "license": "MIT",
  "description": "A lightWeight, robust and easily extensible HTTP(/2,/3)/HTTPS network transport utility",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "@types/express": "^5.0.3",
    "@types/node": "^22.13.1",
    "eslint": "^9.20.0",
    "globals": "^15.14.0",
    "npm-check-updates": "^17.1.15",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typedoc": "^0.27.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.24.0",
    "zx": "^8.3.2"
  },
  "dependencies": {
    "@bull-board/express": "^6.13.1",
    "axios": "^1.7.9",
    "axios-ntlm": "^1.4.3",
    "bottleneck": "^2.19.5",
    "bullmq": "^5.61.0",
    "dotenv": "^16.4.7",
    "express": "^5.1.0",
    "ioredis": "^5.8.1",
    "node-cache": "^5.1.2",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0"
  },
  "files": [
    "dist"
  ]
}
